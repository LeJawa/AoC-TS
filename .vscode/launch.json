  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Build & Debug runner",
      "program": "${workspaceFolder}/dist/runner.js",
      "preLaunchTask": "tsc: build",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "args": ["6", "2025"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug runner",
      "program": "${workspaceFolder}/dist/runner.js",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "args": ["6", "2025"]
    }
  ]
}
